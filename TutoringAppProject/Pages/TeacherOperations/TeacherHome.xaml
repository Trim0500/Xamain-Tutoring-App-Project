<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TutoringAppProject.Pages.TeacherOperations.TeacherHome">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="LogoutToolbar" 
                     Text="Logout"
                     Clicked="LogoutToolbar_Clicked">
        </ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="TutorList" Margin="10,0,10,5" Text="Tutor List" BackgroundColor="LightBlue"  FontSize="Medium" Clicked="TutorList_Clicked"/>
            <Button x:Name="StudentList" Margin="10,0,10,5" Text="Student List" BackgroundColor="LightBlue" FontSize="Medium" Clicked="StudentList_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>